<H2>Main Page</H2>
<a href="/">Main pages</a>
<br>
<H2>Create drone</H2>
<a href="/drones/create">Create drones</a>
<h2>Available drones</h2>
{{#each drones}}
<h1>*{{this.name}}</h1>
<h2>Propellers: {{this.propellers}}</h2>
<h3>Max Speed: {{this.maxSpeed}}</h3>
<a href="/drones/{{this.id}}/edit">edit drone</a>
<form action="/drones/{{this.id}}/delete" method="POST">
<input type="submit" value="Delete">
</form>
{{/each}}